@model Mvc.Models.UsuarioModel

<div class="row">
    <div class="col s12">
        <br />
    <form id="form" class="col s12">
        <input type="hidden" asp-for="Id" />
      <div class="row">
        <div class="input-field col s6">
          <input asp-for="Login" class="validate" />
          <label asp-for="Login"></label>
          <span class="helper-text" data-error=""></span>
        </div>
        <div class="input-field col s6">
                    <input asp-for="Email" class="validate" />
                    <label asp-for="Email"></label>
                    <span class="helper-text" data-error=""></span>
                </div>
      </div>

            <div class="row">
                <div class="input-field col s6">
                    <input asp-for="Telefono" class="validate" />
                      <label asp-for="Telefono"></label>
                    <span class="helper-text" data-error=""></span>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s6">
                    <input asp-for="Password" class="validate" />
                    <label asp-for="Password"></label>
                    <span class="helper-text" data-error=""></span>
                </div>
                <div class="input-field col s6">
                    <input asp-for="ConfirmPassword" class="validate" />
                    <label asp-for="ConfirmPassword"></label>
                    <span class="helper-text" data-error=""></span>
                </div>
            </div>
    </form>
    </div>
  </div>
